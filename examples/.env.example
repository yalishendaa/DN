# Live safety gate (must be 1 + --live to allow real orders)
CONFIRM_LIVE_TRADING=0

# Extended (required when using extended exchange live)
X10_API_KEY=YOUR_EXTENDED_API_KEY
X10_PUBLIC_KEY=0xYOUR_EXTENDED_PUBLIC_KEY
X10_PRIVATE_KEY=0xYOUR_EXTENDED_PRIVATE_KEY
X10_VAULT_ID=YOUR_EXTENDED_VAULT_ID
X10_BUILDER_ID=
X10_ENVIRONMENT=mainnet

# Variational (required when using variational exchange live)
VARIATIONAL_PRIVATE_KEY=0xYOUR_VARIATIONAL_PRIVATE_KEY

# Optional Nado integration (non-default)
NADO_PRIVATE_KEY=0xYOUR_NADO_PRIVATE_KEY
NADO_SUBACCOUNT_NAME=default
